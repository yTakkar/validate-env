{
  "name": "@takkar/validate-env",
  "version": "1.0.9",
  "description": "Validate your env files",
  "keywords": [
    "env",
    "validate",
    "configuration"
  ],
  "main": "./build/index.js",
  "scripts": {
    "example:validate": "node ./build/index.js --sample example/.env.sample --env example/.env.development",
    "lint": "tslint -c ./tslint.json 'src/**/*.ts' -e '(node_modules|build)'",
    "lint:fix": "tslint -p . --fix",
    "test": "jest",
    "ci": "yarn lint && NODE_ENV=test && yarn test",
    "codecov": "codecov",
    "watch-ts": "tsc -w",
    "build-ts": "tsc"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "github.com/yTakkar/validate-env"
  },
  "author": "Faiyaz Shaikh <www.shtakkar@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "chalk": "^2.4.1",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.21",
    "codecov": "^3.1.0",
    "jest": "^23.6.0",
    "jest-watch-typeahead": "^0.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.3.1"
  },
  "bin": "./build/index.js"
}
